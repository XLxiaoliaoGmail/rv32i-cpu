// Immediate Operations Test Program
// Test addi, slti, sltiu, xori, ori, andi, slli, srli, srai
00500093    // addi  x1, x0, 5      # x1 = 5
FFF00113    // addi  x2, x0, -1     # x2 = -1
00A00193    // addi  x3, x0, 10     # x3 = 10

// Test immediate arithmetic and logical operations
00208213    // addi  x4, x1, 2      # x4 = x1 + 2 = 7
0020A293    // slti  x5, x1, 2      # x5 = (x1 < 2) ? 1 : 0 = 0
0020B313    // sltiu x6, x1, 2      # x6 = (x1 < 2) ? 1 : 0 = 0
0020C393    // xori  x7, x1, 2      # x7 = x1 ^ 2 = 7 (5 ^ 2 = 7)
0020E413    // ori   x8, x1, 2      # x8 = x1 | 2 = 7 (5 | 2 = 7) 
0020F493    // andi  x9, x1, 2      # x9 = x1 & 2 = 0 (5 & 2 = 0)

// Test shift immediate operations
00209513    // slli  x10, x1, 2     # x10 = x1 << 2 = 20 (5 << 2 = 20)
0020D593    // srli  x11, x1, 2     # x11 = x1 >> 2 = 1 (5 >> 2 = 1)
4020D613    // srai  x12, x1, 2     # x12 = x1 >> 2 = 1 (5 >> 2 = 1, arithmetic)

// Test upper immediate operations
BEEF0737    // lui   x13, 0xBEEF0   # x13 = 0xBEEF0000

0000006F    // jal x0, 0            # 死循环 